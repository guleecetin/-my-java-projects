public class asalsayı {
// 1 den 1000 e kadar olan sayılardan asal sayıları bulma
	public static boolean asalbulma(int sayı) {
	for(int i=2;i<sayı;i++) {
		if(sayı%i==0) {
			return false;
			
		}
	
	}
	return true;
	}
	public static void main(String[] args) {
	  for(int i=2;i<1000;i++) {
		  if(asalbulma(i)) {
			  System.out.println(i);
		  }
	  }

	}

}
